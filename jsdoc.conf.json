{
  "source": {
    "include": [
      "./assets/js/",
      "./README.md"
    ],
    "includePattern": "\\.(js)$",
    "exclude": [
      "node_modules/"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "El Pollo Loco - Jump'n'Run Game Documentation"
  }
}
